<h1>Stocks#index</h1>
<p>Find me in app/views/stocks/index.html.erb</p>
<% @results.each do |item| %>
<h2><%= item['symbol'] %></h2>
<p>Sector: <%= item['sector'].capitalize %></p>
<p>Market Percent: <%= item['marketPercent'] %>%</p>
<p>Last Sale Price: $<%= item['lastSalePrice'] %></p>
<% end%>
<%= pie_chart({10.day.ago => 5, 15.day.ago => 10})%>

<table border="1px">
  <tr>
    <th>Date</th>
    <th>Headline</th>
    <th>Summary</th>
    <th>Source</th>


  </tr>
<% @news.each do |article| %>
  <tr>
    <td><%= article['datetime'] %></td>
    <td><%= article['headline'] %></td>
    <td><%= article['summary'] %></td>
    <td><a href="<%= article['url']%>" target="_blank"><%= article['source'] %></a></td>
  </tr>
<% end %>

</table>
