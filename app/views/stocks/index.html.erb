<h1>Stocks#index</h1>
<p>Find me in app/views/stocks/index.html.erb</p>
<p><%= @results %></p>
<table border="1px">
  <tr>
    <th>Date</th>
    <th>Headline</th>
    <th>Source</th>

  </tr>
<% @news.each do |article| %>
  <tr>
    <td><%= article['datetime'] %></td>
    <td><%= article['headline'] %></td>
    <td><a href="<%= article['url']%>"><%= article['source'] %></a></td>
  </tr>
<% end %>

</table>
